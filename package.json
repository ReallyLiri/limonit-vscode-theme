{
  "name": "limonit-theme",
  "displayName": "Limonit Theme",
  "description": "A theme inspired by Limonit",
  "version": "0.0.2",
  "publisher": "ReallyLiri",
  "repository": "https://github.com/ReallyLiri/limonit-vscode-theme",
  "icon": "icon.png",
  "scripts": {
    "package": "vsce package --allow-star-activation",
    "pub": "vsce publish"
  },
  "devDependencies": {
    "vsce": "^2.15.0"
  },
  "engines": {
    "vscode": "^1.91.0"
  },
  "categories": [
    "Themes"
  ],
  "main": "./extension.js",
  "activationEvents": [
    "*"
  ],
  "contributes": {
    "themes": [
      {
        "label": "Limonit Theme",
        "uiTheme": "vs-dark",
        "path": "./themes/theme.json"
      }
    ],
    "commands": [
      {
        "command": "limonitTheme.openWebview",
        "title": "Open Limonit Webview"
      }
    ]
  }
}
