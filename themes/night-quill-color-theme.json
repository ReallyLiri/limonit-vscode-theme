  "tokenColors": [
    {
      "scope": ["comment", "punctuation.definition.comment"],
      "settings": {
        "foreground": "#967A60",
        "fontStyle": "italic"
      }
    },
    {
      "scope": ["string", "constant.other.symbol", "constant.other.key"],
      "settings": {
        "foreground": "#BFA68F"
      }
    },
    {
      "scope": ["constant.numeric", "constant.language", "constant.character", "constant.escape"],
      "settings": {
        "foreground": "#6A4628"
      }
    },
    {
      "scope": ["keyword", "storage.type", "storage.modifier", "keyword.control"],
      "settings": {
        "foreground": "#8C6B4F",
        "fontStyle": "italic"
      }
    },
    {
      "scope": ["entity.name.function", "meta.function-call", "variable.function", "support.function", "keyword.other.special-method"],
      "settings": {
        "foreground": "#5E3B22",
        "fontStyle": "bold"
      }
    },
    {
      "scope": ["entity.name.class", "support.class", "support.type", "entity.other.inherited-class"],
      "settings": {
        "foreground": "#593520"
      }
    },
    {
      "scope": ["variable", "variable.parameter", "variable.language"],
      "settings": {
        "foreground": "#733E32"
      }
    },
    {
      "scope": ["invalid", "invalid.illegal"],
      "settings": {
        "foreground": "#261711",
        "fontStyle": "underline"
      }
    },
    {
      "scope": ["markup.heading", "markup.heading.markdown", "markup.heading entity.name"],
      "settings": {
        "foreground": "#BFA68F",
        "fontStyle": "bold"
      }
    },
    {
      "scope": ["markup.bold", "markup.bold string"],
      "settings": {
        "foreground": "#8C6B4F",
        "fontStyle": "bold"
      }
    },
    {
      "scope": ["markup.italic"],
      "settings": {
        "foreground": "#967A60",
        "fontStyle": "italic"
      }
    },
    {
      "scope": ["markup.underline"],
      "settings": {
        "foreground": "#BFA68F",
        "fontStyle": "underline"
      }
    },
    {
      "scope": ["entity.other.attribute-name"],
      "settings": {
        "foreground": "#8C6B4F"
      }
    },
    {
      "scope": ["meta.separator"],
      "settings": {
        "foreground": "#967A60",
        "fontStyle": "bold"
      }
    },
    {
      "scope": ["markup.quote"],
      "settings": {
        "foreground": "#967A60",
        "fontStyle": "italic"
      }
    },
    {
      "scope": ["markup.inserted"],
      "settings": {
        "foreground": "#8C6B4F"
      }
    },
    {
      "scope": ["markup.deleted"],
      "settings": {
        "foreground": "#733E32"
      }
    },
    {
      "scope": ["markup.changed"],
      "settings": {
        "foreground": "#6A4628"
      }
    },
    {
      "scope": ["string.regexp", "constant.character.escape"],
      "settings": {
        "foreground": "#5E3B22"
      }
    },
    {
      "scope": ["*url*", "*link*", "*uri*"],
      "settings": {
        "foreground": "#BFA68F",
        "fontStyle": "underline"
      }
    },
    {
      "scope": ["meta.block variable.other"],
      "settings": {
        "foreground": "#BFA68F"
      }
    },
    {
      "scope": ["support.constant", "support.variable"],
      "settings": {
        "foreground": "#967A60"
      }
    },
    {
      "scope": ["entity.name.tag", "meta.tag.sgml"],
      "settings": {
        "foreground": "#8C6B4F"
      }
    },
    {
      "scope": ["entity.other.attribute-name.class"],
      "settings": {
        "foreground": "#5E3B22"
      }
    },
    {
      "scope": ["source.css support.type.property-name", "source.sass support.type.property-name", "source.scss support.type.property-name", "source.less support.type.property-name", "source.stylus support.type.property-name", "source.postcss support.type.property-name"],
      "settings": {
        "foreground": "#BFA68F"
      }
    }
  ]
}
